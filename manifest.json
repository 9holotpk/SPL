{
  "content_scripts": [
    {
      "matches": [
        "*://open.spotify.com/*"
      ], 
      "js": [
        "contentscript.js"
      ]
    }
  ], 
  "browser_action": {
    "default_icon": "icons/spotify_128.png", 
    "default_title": "Spotify Launcher"
  }, 
  "description": "Fast Launcher for Spotify Web Player", 
  "icons": {
    "128": "icons/spotify_128.png", 
    "48": "icons/spotify_48.png", 
    "16": "icons/spotify_16.png"
  }, 
  "background": {
    "scripts": [
      "background.js"
    ]
  }, 
  "name": "Spotify Launcher", 
  "options_ui": {
    "chrome_style": true, 
    "page": "options.html", 
    "open_in_tab": false
  }, 
  "version": "0.1", 
  "options_page": "options.html", 
  "manifest_version": 2, 
  "permissions": [
    "tabs", 
    "https://open.spotify.com/", 
    "storage"
  ], 
  "developer": {
    "url": "http://twitter.com/9holoTpk", 
    "name": "Sontaya T. Wichakot"
  }
}